#!/bin/bash
## install tmux config 
printf %b "\n-- installing tmux & configs ... \n"; 
mkdir -m 775 -p $HOME/.config/tmux 2>/dev/null; 
ln -s ""$HOME/zz/c/tmux/{hl.tmux,tmux.conf,tmuxbg.sh,tmuxcompletions.sh}" " -t $HOME/.config/tmux --backup --suffix "$(date +%y%m%d%H%M%S)" 2>/dev/null; 
# ln -s $HOME/zz/c/tmux/plugins/* -t $HOME/.config/tmux/plugins 2>/dev/null; 
printf %b "\n-- done!\n\n"; 
